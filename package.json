{
    "name": "myinsurancebuddy-monorepo",
    "version": "0.0.1",
    "private": true,
    "packageManager": "pnpm@10.25.0",
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "scripts": {
        "postinstall": "prisma generate --schema=packages/db/prisma/schema.prisma",
        "build": "turbo run build",
        "dev": "turbo run dev",
        "lint": "turbo run lint",
        "format": "prettier --write \"**/*.{ts,tsx,md}\"",
        "start:web": "cd apps/web && pnpm start",
        "start:admin": "cd apps/admin && pnpm start"
    },
    "devDependencies": {
        "turbo": "latest",
        "prettier": "latest",
        "prisma": "^5.22.0"
    },
    "dependencies": {
        "@prisma/client": "^5.22.0"
    }
}